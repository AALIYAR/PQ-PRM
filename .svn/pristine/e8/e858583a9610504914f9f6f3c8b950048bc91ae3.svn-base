                              rendered="#{!(bindings.ActivitiesIterator.rangeStart+bindings.ActivitiesIterator.currentRowIndexInRange+1 lt bindings.ActivitiesIterator.estimatedRowCount)}"
                              id="img2"/>
                    <af:commandToolbarButton actionListener="#{bindings.LastActivities.execute}"
                                             icon="/afr/shuttlerightall_ena.png"
                                             shortDesc="#{nls['LAST_ROW']}"
                                             id="ActLast" partialSubmit="true"
                                             rendered="#{bindings.ActivitiesIterator.rangeStart+bindings.ActivitiesIterator.currentRowIndexInRange+1 lt bindings.ActivitiesIterator.estimatedRowCount}"></af:commandToolbarButton>
