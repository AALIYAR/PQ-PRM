  <SQLQuery>
    <![CDATA[SELECT PrmCampaignTitle.ID, 
       PrmCampaignTitle.PRM_CAMPAIGN_ID, 
       PrmCampaignTitle.TITLE_ID, 
       PrmCampaignTitle.ADDED_DATE, 
       PrmCampaignTitle.REMOVED_DATE, 
       PrmCampaignTitle.CREATED_BY, 
       PrmCampaignTitle.CREATED_DATE, 
       PrmCampaignTitle.LAST_UPDATED_BY, 
       PrmCampaignTitle.LAST_UPDATED_DATE,
       TraxTitle.TARGET_TRACKING_NO,
       TraxTitle.CATALOG_NUMBER,
       TraxTitle.LONG_TITLE,
       TraxContentOwner.CONTENT_OWNER_NO Account_No,
       TraxContentOwner.NAME Account_Name,
        ( SELECT  LISTAGG(index_name, ',') WITHIN GROUP (ORDER BY index_name) AS indexs
                FROM   trax_title_index_inclusion 
               where title_id = tt.id
               group by title_id) external_index
FROM PRM_CAMPAIGN_TITLE PrmCampaignTitle
    INNER JOIN TRAX_TITLE TraxTitle
        INNER JOIN TRAX_CONTENT_OWNER TraxContentOwner
            ON TraxTitle.CONTENT_OWNER_ID = TraxContentOwner.ID
        ON PrmCampaignTitle.TITLE_ID = TraxTitle.ID

INNER JOIN TRAX_TITLE tt 
on   tt.content_owner_id = TraxContentOwner.id and tt.active_flag = 'Y']]>
  </SQLQuery>
