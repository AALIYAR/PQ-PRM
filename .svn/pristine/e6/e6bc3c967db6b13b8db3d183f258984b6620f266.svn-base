<?xml version='1.0' encoding="UTF-8"?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
          xmlns:trh="http://myfaces.apache.org/trinidad/html"
          xmlns:tr="http://myfaces.apache.org/trinidad"
          xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:pageTemplate id="pt"
                   viewId="/common/pageTemplates/JhsRegionTemplateNoStretch.jspx">
    <f:facet name="pageContent">
      <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
      <af:panelGroupLayout id="pcpg" layout="scroll">
        <af:panelGroupLayout layout="vertical" id="ActTopLc"
                             partialTriggers=" ActSaveButton0 ActCancelButton0 asAreaActSearchOptions qsAreaActSearchOptions"
                             rendered="#{jhsUserRoles['AM, PM, AST, GCA, ADMIN, RO']}">
          <f:facet name="separator">
            <af:spacer width="10" height="10" id="sp0"/>
          </f:facet>
          <!-- Pushed "disabled" on stretchContext, now: Stretching disabled on group level. -->
          <af:panelHeader id="ph0" text="#{nls['TABLE_TITLE_ACTIVITIES']}"
                          helpTopicId="RBH_ACTIVITIES">
            <f:facet name="toolbar">
              <af:toolbox id="tbox0">
                <af:toolbar id="tbar0">
                  <af:group id="gr0">
                    <af:commandButton actionListener="#{CreateActivities.execute}"
                                      action="CreateAct"
                                      textAndAccessKey="#{nls['NEW_BUTTON_LABEL_ACTIVITIES']}"
                                      rendered="#{(true) and (jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                      id="ActNewButton"></af:commandButton>
                  </af:group>
                  <af:group id="gr1"></af:group>
                  <af:group id="gr2">
                    <af:commandButton action="#{SaveActivities.execute}"
                                      rendered="#{(jhsUserRoles['AM,AST,PM,GCA,ADMIN']) and (!pageFlowScope.hideSaveButton)}"
                                      textAndAccessKey="#{nls['SAVE_BUTTON_LABEL']}"
                                      id="ActSaveButton0" partialSubmit="true"></af:commandButton>
                    <af:commandButton textAndAccessKey="#{nls['CANCEL']}"
                                      immediate="true" partialSubmit="true"
                                      rendered="#{!pageFlowScope.hideCancelButton}"
                                      actionListener="#{jhsRollback.execute}"
                                      action="Cancel" id="ActCancelButton0"></af:commandButton>
                  </af:group>
                </af:toolbar>
              </af:toolbox>
            </f:facet>
            <f:facet name="context"></f:facet>
            <af:panelGroupLayout id="modelSearchAreaAct"
                                 rendered="#{!pageFlowScope.createModes.CreateActivities and !pageFlowScope.hideSearchArea}"
                                 partialTriggers=" asAreaActSearchOptions:qsButtonAct qsAreaActSearchOptions:asButtonAct ">
              <f:attribute name="JhsIgnoreChanges" value="true"/>
              <af:query id="asAreaActSearchOptions"
                        headerText="#{nls['ADVANCED_SEARCH']}" disclosed="true"
                        value="#{bindings.ActivitiesIteratorTraxActivityViewCriteriaQuery.queryDescriptor}"
                        model="#{bindings.ActivitiesIteratorTraxActivityViewCriteriaQuery.queryModel}"
                        queryListener="#{bindings.ActivitiesIteratorTraxActivityViewCriteriaQuery.processQuery}"
                        queryOperationListener="#{bindings.ActivitiesIteratorTraxActivityViewCriteriaQuery.processQueryOperation}"
                        rendered="#{!pageFlowScope.searchActivities.quickSearchMode}"
                        rows="1" labelAlignment="top" maxColumns="4">
                <f:facet name="toolbar">
                  <af:commandButton partialSubmit="true" id="qsButtonAct"
                                    immediate="true"
                                    textAndAccessKey="#{nls['QUICK_SEARCH']}"
                                    action="#{pageFlowScope.searchActivities.switchToQuickSearchMode}"/>
                </f:facet>
              </af:query>
              <af:quickQuery id="qsAreaActSearchOptions"
                             label="#{nls['FILTER_BY']}"
                             value="#{bindings.ActivitiesIteratorTraxActivityQuickQueryQuery.queryDescriptor}"
                             model="#{bindings.ActivitiesIteratorTraxActivityQuickQueryQuery.queryModel}"
                             queryListener="#{bindings.ActivitiesIteratorTraxActivityQuickQueryQuery.processQuery}"
                             rendered="#{pageFlowScope.searchActivities.quickSearchMode}">
                <f:facet name="end">
                  <af:commandLink id="asButtonAct"
                                  textAndAccessKey="#{nls['ADVANCED_SEARCH']}"
                                  partialSubmit="true" immediate="true"
                                  action="#{pageFlowScope.searchActivities.switchToAdvancedSearchMode}"/>
                </f:facet>
              </af:quickQuery>
            </af:panelGroupLayout>
          </af:panelHeader>
          <af:panelGroupLayout layout="vertical" id="pgl0">
            <af:panelCollection id="ActPC" featuresOff=" "
                                styleClass="AFStretchWidth">
              <f:facet name="menus">
                <af:menu textAndAccessKey="#{nls['ACTION_MENU_LABEL']}"
                         id="menu0">
                  <af:menu textAndAccessKey="#{nls['GO_TO_MENU_LABEL']}"
                           id="menu1">
                    <af:commandMenuItem id="mi0"
                                        textAndAccessKey="#{nls['DETAIL_VIEW_BUTTON_LABEL']}"
                                        action="ActivitiesDetails"></af:commandMenuItem>
                    <af:group id="Acttbb"
                              rendered="#{ (bindings.ActivitiesIterator.viewObject!=null and bindings.ActivitiesIterator.viewObject.currentRow!=null)}">
                      <af:commandMenuItem actionListener="#{bindings.FirstActivities.execute}"
                                          id="ActFirst" partialSubmit="true"
                                          icon="/afr/shuttleleftall_ena.png"
                                          textAndAccessKey="#{nls['FIRST_ROW']}"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.PreviousActivities.execute}"
                                          icon="/afr/shuttleleft_ena.png"
                                          textAndAccessKey="#{nls['PREVIOUS_ROW']}"
                                          id="ActPrevious" partialSubmit="true"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.NextActivities.execute}"
                                          icon="/afr/shuttleright_ena.png"
                                          textAndAccessKey="#{nls['NEXT_ROW']}"
                                          id="ActNext" partialSubmit="true"></af:commandMenuItem>
                      <af:commandMenuItem actionListener="#{bindings.LastActivities.execute}"
                                          icon="/afr/shuttlerightall_ena.png"
                                          textAndAccessKey="#{nls['LAST_ROW']}"
                                          id="ActLast" partialSubmit="true"></af:commandMenuItem>
                    </af:group>
                  </af:menu>
                  <af:commandMenuItem partialSubmit="true"
                                      textAndAccessKey="#{nls['EXPORT']}"
                                      icon="/jheadstart/images/excel.png"
                                      immediate="true" id="cmi0">
                    <af:exportCollectionActionListener exportedId="ActTab"
                                                       type="excelHTML"
                                                       filename="Activities.xls"/>
                  </af:commandMenuItem>
                </af:menu>
              </f:facet>
              <f:facet name="toolbar">
                <af:toolbar id="tbar1">
                  <af:commandImageLink immediate="true" partialSubmit="true"
                                       id="cl0" shortDesc="#{nls['EXPORT']}"
                                       icon="/jheadstart/images/excel.png">
                    <af:exportCollectionActionListener exportedId="ActTab"
                                                       type="excelHTML"
                                                       filename="Activities.xls"/>
                  </af:commandImageLink>
                  <af:commandToolbarButton text="Print" id="ActPrint"
                                           shortDesc="Opens a new tab with printer friendly format.">
                    <af:showPrintablePageBehavior/>
                  </af:commandToolbarButton>
                </af:toolbar>
              </f:facet>
              <f:facet name="statusbar">
                <af:toolbar>
                  <af:outputText value="#{bindings.ActivitiesIterator.estimatedRowCount} rows found"/>
                </af:toolbar>
              </f:facet>
              <af:table id="ActTab" contentDelivery="immediate"
                        value="#{bindings.ActivitiesTable.collectionModel}"
                        columnStretching="blank" rowBandingInterval="0"
                        editingMode="editAll" columnSelection="multiple"
                        contextMenuSelect="true" rowSelection="single"
                        selectionListener="#{pageFlowScope.ActivitiesTable.makeCurrent}"
                        selectedRowKeys="#{pageFlowScope.ActivitiesTable.selectedRow}"
                        rowDisclosureListener="#{pageFlowScope.ActivitiesTable.discloseRow}"
                        var="row" varStatus="status"
                        rows="#{bindings.ActivitiesTable.rangeSize}"
                        autoHeightRows="#{bindings.ActivitiesTable.estimatedRowCount}"
                        fetchSize="#{bindings.ActivitiesTable.rangeSize}"
                        emptyText="#{nls['NO_ROWS_FOUND']}"
                        queryListener="#{bindings.ActivitiesIteratorTraxActivityQuickQueryQuery.processQuery}"
                        filterModel="#{bindings.ActivitiesIteratorTraxActivityQuickQueryQuery.queryDescriptor}"
                        filterVisible="true"
                        binding="#{pageFlowScope.ActivitiesTable.table}"
                        partialTriggers=":::qsAreaActSearchOptions :::asAreaActSearchOptions :::ActPC ::ActFirst ::ActPrevious ::ActNext ::ActLast"
                        styleClass="AFStretchWidth"
                        summary="#{nls['TABLE_TITLE_ACTIVITIES']}">
                <f:facet name="contextMenu">
                  <af:popup id="ActTablePopup" childCreation="deferred">
                    <af:menu id="menu2">
                      <af:menu text="#{nls['GO_TO_MENU_LABEL']}" id="menu3">
                        <af:commandMenuItem id="mi1"
                                            textAndAccessKey="#{nls['DETAIL_VIEW_BUTTON_LABEL']}"
                                            action="ActivitiesDetails"></af:commandMenuItem>
                      </af:menu>
                    </af:menu>
                  </af:popup>
                </f:facet>
                <af:column id="ActDesCol" sortable="true" filterable="true"
                           noWrap="true" width="260" sortProperty="Description"
                           rowHeader="false"
                           helpTopicId="RBH_ACTDESCRIPTIONTABLE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.Description.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.Description.mandatory}"
                                    id="ol0"/>
                  </f:facet>
                  <af:panelLabelAndMessage shortDesc="#{bindings.ActivitiesTable.hints.Description.tooltip}">
                    <af:inputText id="ActDes"
                                  value="#{row.bindings.Description.inputValue}"
                                  label="#{bindings.ActivitiesTable.hints.Description.label}"
                                  required="#{bindings.ActivitiesTable.hints.Description.mandatory}"
                                  rows="1"
                                  columns="#{bindings.ActivitiesTable.hints.Description.displayWidth}"
                                  maximumLength="4000"
                                  readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                  autoSubmit="true" simple="true"
                                  partialTriggers="ActDescriptionTableEditorLink ActDescriptionTableEditorText">
                      <f:validator binding="#{row.bindings.Description.validator}"/>
                    </af:inputText>
                    <af:commandLink id="ActDescriptionTableEditorLink"
                                    rendered="#{!(!(jhsUserRoles['AM,AST,PM,GCA,ADMIN']))}"
                                    immediate="true" partialSubmit="true">
                      <af:showPopupBehavior popupId="ActDescriptionTablePopup"/>
                      <af:image source="/jheadstart/images/editor.gif"
                                id="ActDescriptionTableIcon"
                                clientComponent="true"/>
                    </af:commandLink>
                  </af:panelLabelAndMessage>
                  <af:popup id="ActDescriptionTablePopup"
                            contentDelivery="lazyUncached"
                            eventContext="launcher" childCreation="deferred">
                    <af:dialog modal="true" resize="on" title="Text Editor"
                               id="dialog0">
                      <af:inputText rows="10" columns="100"
                                    value="#{row.bindings.Description.inputValue}"
                                    simple="true" maximumLength="4000"
                                    id="ActDescriptionTableEditorText"/>
                    </af:dialog>
                  </af:popup>
                </af:column>
                <af:column id="ActTyCol" sortable="true" filterable="true"
                           noWrap="true" width="120" sortProperty="Type"
                           rowHeader="false" helpTopicId="RBH_ACTTYPE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.Type.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.Type.mandatory}"
                                    id="ol1"/>
                  </f:facet>
                  <af:inputText id="cl1" value="#{row.bindings.Type.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.Type.label}"
                                required="#{bindings.ActivitiesTable.hints.Type.mandatory}"
                                rows="#{bindings.ActivitiesTable.hints.Type.displayHeight}"
                                columns="#{bindings.ActivitiesTable.hints.Type.displayWidth}"
                                maximumLength="40"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.Type.tooltip}"
                                autoSubmit="true"
                                valueChangeListener="#{ActTypeDependsOnItem.updateModelValue}"
                                rendered="#{adfFacesContext.outputMode eq 'printable'}">
                    <f:validator binding="#{row.bindings.Type.validator}"/>
                  </af:inputText>
                  <af:commandLink id="cl2"
                                  shortDesc="#{bindings.ActivitiesTable.hints.Type.tooltip}"
                                  rendered="#{adfFacesContext.outputMode != 'printable'}"
                                  action="ActivitiesDetails" text="#{row.Type}"></af:commandLink>
                </af:column>
                <af:column id="ActSTyCol" sortable="true" filterable="true"
                           noWrap="true" width="120" sortProperty="SubType"
                           rowHeader="false" helpTopicId="RBH_ACTSUBTYPE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.SubType.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.SubType.mandatory}"
                                    id="ol2"/>
                  </f:facet>
                  <af:selectOneChoice id="ActSTy"
                                      value="#{row.bindings.SubType.inputValue}"
                                      label="#{bindings.ActivitiesTable.hints.SubType.label}"
                                      required="#{bindings.ActivitiesTable.hints.SubType.mandatory}"
                                      readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                      shortDesc="#{bindings.ActivitiesTable.hints.SubType.tooltip}">
                    <f:selectItems value="#{row.bindings.SubType.items}"/>
                  </af:selectOneChoice>
                </af:column>
                <af:column id="ActStCol" sortable="true" filterable="true"
                           noWrap="true" width="120" sortProperty="Status"
                           rowHeader="false" helpTopicId="RBH_ACTSTATUS"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.Status.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.Status.mandatory}"
                                    id="ol3"/>
                  </f:facet>
                  <af:selectOneChoice id="ActSt"
                                      value="#{row.bindings.Status.inputValue}"
                                      label="#{bindings.ActivitiesTable.hints.Status.label}"
                                      required="#{bindings.ActivitiesTable.hints.Status.mandatory}"
                                      readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                      shortDesc="#{bindings.ActivitiesTable.hints.Status.tooltip}">
                    <f:selectItems value="#{row.bindings.Status.items}"/>
                  </af:selectOneChoice>
                </af:column>
                <af:column id="ActPriCol" sortable="true" filterable="true"
                           noWrap="true" width="120" sortProperty="Priority"
                           rowHeader="false" helpTopicId="RBH_ACTPRIORITY"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.Priority.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.Priority.mandatory}"
                                    id="ol4"/>
                  </f:facet>
                  <af:selectOneChoice id="ActPri"
                                      value="#{row.bindings.Priority.inputValue}"
                                      label="#{bindings.ActivitiesTable.hints.Priority.label}"
                                      required="#{bindings.ActivitiesTable.hints.Priority.mandatory}"
                                      readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                      shortDesc="#{bindings.ActivitiesTable.hints.Priority.tooltip}">
                    <f:selectItems value="#{row.bindings.Priority.items}"/>
                  </af:selectOneChoice>
                </af:column>
                <af:column id="ActASDCol" sortable="true" filterable="true"
                           noWrap="true" width="100"
                           sortProperty="ActualStartDate" rowHeader="false"
                           helpTopicId="RBH_ACTACTUALSTARTDATE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.ActualStartDate.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.ActualStartDate.mandatory}"
                                    id="ol5"/>
                  </f:facet>
                  <f:facet name="filter">
                    <af:inputDate value="#{status.filterCriteria.ActualStartDate}"
                                  id="id0">
                      <af:convertDateTime pattern="#{nls.datepattern}"/>
                    </af:inputDate>
                  </f:facet>
                  <af:inputDate id="ActASD"
                                value="#{row.bindings.ActualStartDate.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.ActualStartDate.label}"
                                required="#{bindings.ActivitiesTable.hints.ActualStartDate.mandatory}"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.ActualStartDate.tooltip}">
                    <af:convertDateTime pattern="#{nls.datepattern}"/>
                  </af:inputDate>
                </af:column>
                <af:column id="ActCoDCol" sortable="true" filterable="true"
                           noWrap="true" width="100"
                           sortProperty="CompletionDate" rowHeader="false"
                           helpTopicId="RBH_ACTCOMPLETIONDATE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.CompletionDate.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.CompletionDate.mandatory}"
                                    id="ol6"/>
                  </f:facet>
                  <f:facet name="filter">
                    <af:inputDate value="#{status.filterCriteria.CompletionDate}"
                                  id="id1">
                      <af:convertDateTime pattern="#{nls.datepattern}"/>
                    </af:inputDate>
                  </f:facet>
                  <af:inputDate id="ActCoD"
                                value="#{row.bindings.CompletionDate.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.CompletionDate.label}"
                                required="#{bindings.ActivitiesTable.hints.CompletionDate.mandatory}"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.CompletionDate.tooltip}">
                    <af:convertDateTime pattern="#{nls.datepattern}"/>
                  </af:inputDate>
                </af:column>
                <af:column id="ActDDCol" sortable="true" filterable="true"
                           noWrap="true" width="100" sortProperty="DueDate"
                           rowHeader="false" helpTopicId="RBH_ACTDUEDATE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.DueDate.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.DueDate.mandatory}"
                                    id="ol7"/>
                  </f:facet>
                  <f:facet name="filter">
                    <af:inputDate value="#{status.filterCriteria.DueDate}"
                                  id="id2">
                      <af:convertDateTime pattern="#{nls.datepattern}"/>
                    </af:inputDate>
                  </f:facet>
                  <af:inputDate id="ActDD"
                                value="#{row.bindings.DueDate.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.DueDate.label}"
                                required="#{bindings.ActivitiesTable.hints.DueDate.mandatory}"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.DueDate.tooltip}">
                    <af:convertDateTime pattern="#{nls.datepattern}"/>
                  </af:inputDate>
                </af:column>
                <af:column id="ActESDCol" sortable="true" filterable="true"
                           noWrap="true" width="100"
                           sortProperty="ExpectedStartDate" rowHeader="false"
                           helpTopicId="RBH_ACTEXPECTEDSTARTDATE"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.ExpectedStartDate.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.ExpectedStartDate.mandatory}"
                                    id="ol8"/>
                  </f:facet>
                  <f:facet name="filter">
                    <af:inputDate value="#{status.filterCriteria.ExpectedStartDate}"
                                  id="id3">
                      <af:convertDateTime pattern="#{nls.datepattern}"/>
                    </af:inputDate>
                  </f:facet>
                  <af:inputDate id="ActESD"
                                value="#{row.bindings.ExpectedStartDate.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.ExpectedStartDate.label}"
                                required="#{bindings.ActivitiesTable.hints.ExpectedStartDate.mandatory}"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.ExpectedStartDate.tooltip}">
                    <af:convertDateTime pattern="#{nls.datepattern}"/>
                  </af:inputDate>
                </af:column>
                <af:column id="ActHSCol" sortable="true" filterable="true"
                           noWrap="true" width="100" sortProperty="HoursSpent"
                           align="right" rowHeader="false"
                           helpTopicId="RBH_ACTHOURSSPENT"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.HoursSpent.label}"
                                    showRequired="#{bindings.ActivitiesTable.hints.HoursSpent.mandatory}"
                                    id="ol9"/>
                  </f:facet>
                  <af:inputText id="ActHS"
                                value="#{row.bindings.HoursSpent.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.HoursSpent.label}"
                                required="#{bindings.ActivitiesTable.hints.HoursSpent.mandatory}"
                                rows="#{bindings.ActivitiesTable.hints.HoursSpent.displayHeight}"
                                columns="#{bindings.ActivitiesTable.hints.HoursSpent.displayWidth}"
                                maximumLength="8"
                                readOnly="#{!(jhsUserRoles['AM,AST,PM,GCA,ADMIN'])}"
                                shortDesc="#{bindings.ActivitiesTable.hints.HoursSpent.tooltip}">
                    <f:validator binding="#{row.bindings.HoursSpent.validator}"/>
                    <af:convertNumber groupingUsed="false"
                                      pattern="#{bindings.ActivitiesTable.hints.HoursSpent.format}"/>
                  </af:inputText>
                </af:column>
                <af:column id="ActANoCol" sortable="true" filterable="true"
                           noWrap="true" width="100"
                           sortProperty="ContentOwnerNo" align="right"
                           rowHeader="false" helpTopicId="RBH_ACTCONTENTOWNERNO"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.ContentOwnerNo.label}"
                                    id="ol10"/>
                  </f:facet>
                  <af:inputText id="ActANo"
                                value="#{row.bindings.ContentOwnerNo.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.ContentOwnerNo.label}"
                                required="#{bindings.ActivitiesTable.hints.ContentOwnerNo.mandatory}"
                                rows="#{bindings.ActivitiesTable.hints.ContentOwnerNo.displayHeight}"
                                columns="#{bindings.ActivitiesTable.hints.ContentOwnerNo.displayWidth}"
                                maximumLength="38" readOnly="true"
                                shortDesc="#{bindings.ActivitiesTable.hints.ContentOwnerNo.tooltip}">
                    <f:validator binding="#{row.bindings.ContentOwnerNo.validator}"/>
                    <af:convertNumber groupingUsed="false"
                                      pattern="#{bindings.ActivitiesTable.hints.ContentOwnerNo.format}"/>
                  </af:inputText>
                </af:column>
                <af:column id="ActANmCol" sortable="true" filterable="true"
                           noWrap="true" width="240" sortProperty="AccountName"
                           rowHeader="false" helpTopicId="RBH_ACTACCOUNTNAME"
                           filterFeatures="caseInsensitive">
                  <f:facet name="header">
                    <af:outputLabel value="#{bindings.ActivitiesTable.hints.AccountName.label}"
                                    id="ol11"/>
                  </f:facet>
                  <af:inputText id="cl3"
                                value="#{row.bindings.AccountName.inputValue}"
                                label="#{bindings.ActivitiesTable.hints.AccountName.label}"
                                required="#{bindings.ActivitiesTable.hints.AccountName.mandatory}"
                                rows="#{bindings.ActivitiesTable.hints.AccountName.displayHeight}"
                                columns="#{bindings.ActivitiesTable.hints.AccountName.displayWidth}"
                                maximumLength="255" readOnly="true"
                                shortDesc="#{bindings.ActivitiesTable.hints.AccountName.tooltip}"
                                rendered="#{adfFacesContext.outputMode eq 'printable'}">
                    <f:validator binding="#{row.bindings.AccountName.validator}"/>
                  </af:inputText>
                  <af:commandLink id="ActANm"
                                  text="#{row.bindings.AccountName.attributeValue}"
                                  shortDesc="#{bindings.ActivitiesTable.hints.AccountName.tooltip}"
                                  action="LaunchTab"
                                  rendered="#{adfFacesContext.outputMode != 'printable'}">
                    <af:setActionListener from="Accounts"
                                          to="#{jhsDynTabLauncher.taskFlowName}"/>
                    <af:setActionListener from="#{bindings.ActAccountId.inputValue}"
                                          to="#{jhsDynTabLauncher.tabUniqueIdentifier}"/>
                    <af:setActionListener from="Account - #{bindings.ActContentOwnerNo.inputValue}"
                                          to="#{jhsDynTabLauncher.label}"/>
                    <af:setActionListener from="#{bindings.ActAccountId.inputValue}"
