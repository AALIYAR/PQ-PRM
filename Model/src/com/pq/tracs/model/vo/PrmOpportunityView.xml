  <SQLQuery>
    <![CDATA[SELECT PrmOpportunity.ID, 
       PrmOpportunity.CONTENT_OWNER_ID, 
       PrmOpportunity.OPPORTUNITY_NO,
       PrmOpportunity.NAME, 
       PrmOpportunity.DESCRIPTION, 
       PrmOpportunity.SUMMARY, 
       PrmOpportunity.CURRENT_STATUS,
       TraxAccount.NAME AccountName,
       JhsUsers.DISPLAY_NAME Alliance_Manager,
       TraxAccount.SITE Account_Site,
       TraxAccount.CONTENT_OWNER_NO Account_Number, 
       PrmOpportunity.PROBABILITY_PCT, 
       PrmOpportunity.PRIORITY, 
       PrmOpportunity.EXPECTED_PIPELINES, 
       PrmOpportunity.ESTIMATED_CLOSE_QUARTER, 
       PrmOpportunity.AGREEMENT_RECEIVED_DATE, 
       PrmOpportunity.EXCLUSIVITY_SOUGHT_OBTAINED, 
       PrmOpportunity.OPPORTUNITY_TYPE, 
       PrmOpportunity.REASON_LOST, 
       PrmOpportunity.DOCUMENT_SIGNED, 
       PrmOpportunity.CREATED_BY, 
       PrmOpportunity.CREATED_DATE, 
       PrmOpportunity.LAST_UPDATED_BY, 
       PrmOpportunity.LAST_UPDATED_DATE,
       TraxAccount.ID AccountId,
       PrmOpportunity.SUMMON_DEEP_INDEXING_FLAG,
       TraxAddress.CITY,
       TraxAddress.COUNTRY,
       PrmOpportunity.CURRENT_PRE_STATUS,
       PrmOpportunity.CURRENT_CATALOGING_STATUS,
       PrmOpportunity.CURRENT_AGREEMENT_STATUS,
       PrmOpportunity.REASON_LOST AS REASON_LOST1, 
       PrmOpportunity.CURRENT_STATUS AS CURRENT_STATUS1,
       PrmOpportunity.AUTO_ADD_TARGETS_FLAG,  
       PrmOpportunity.OPEN_ACCESS_FLAG
FROM PRM_OPPORTUNITY PrmOpportunity
    INNER JOIN TRAX_CONTENT_OWNER TraxAccount
        LEFT OUTER JOIN TRAX_ADDRESS TraxAddress
            ON TraxAccount.ID = TraxAddress.CONTENT_OWNER_ID
            AND TraxAddress.PRIMARY_FLAG = 'Y'
        ON PrmOpportunity.CONTENT_OWNER_ID = TraxAccount.ID
    LEFT OUTER JOIN PRM_OPPORTUNITY_ALLIANCE_MGR PrmOpportunityAllianceManager
        INNER JOIN PRM_ALLIANCE_MANAGER PrmAllianceManager
            INNER JOIN JHS_USERS JhsUsers
                ON PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
            ON PrmOpportunityAllianceManager.PRM_ALLIANCE_MANAGER_ID = PrmAllianceManager.ID
        ON PrmOpportunity.ID = PrmOpportunityAllianceManager.PRM_OPPORTUNITY_ID
        AND PrmOpportunityAllianceManager.END_DATE IS NULL
    INNER JOIN TRAX_LOOKUP_CODE LookupCode
        ON PrmOpportunity.OPPORTUNITY_TYPE = LookupCode.INTERNAL_VALUE
        AND LookupCode.LOOKUP_CODE_SET_ID = 210]]>
  </SQLQuery>
