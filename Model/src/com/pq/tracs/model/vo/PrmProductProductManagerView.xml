  <SQLQuery>
    <![CDATA[SELECT PrmTargetedProductManager.ID, 
        PrmTargetedProductManager.PRM_USER_ID, 
        PrmTargetedProductManager.PRM_TARGETED_PRODUCT_ID, 
       PrmTargetedProductManager.ADDED_DATE, 
       PrmTargetedProductManager.REMOVED_DATE, 
