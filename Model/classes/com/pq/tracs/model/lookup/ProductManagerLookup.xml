  <SQLQuery>
    <![CDATA[SELECT PrmAllianceManager.ID,
       JhsUsers.FIRST_NAME,
       JhsUsers.LAST_NAME,
       JhsUsers.DISPLAY_NAME,
       JhsUsers.USERNAME, 
       PrmAllianceManager.START_DATE, 
       PrmAllianceManager.END_DATE, 
       PrmAllianceManager.CREATED_BY, 
       PrmAllianceManager.CREATED_DATE, 
       PrmAllianceManager.LAST_UPDATED_BY, 
       PrmAllianceManager.LAST_UPDATED_DATE
FROM PRM_ALLIANCE_MANAGER PrmAllianceManager, JHS_USERS JhsUsers
WHERE PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
AND PrmAllianceManager.USER_TYPE IN ('PM')
AND PrmAllianceManager.ACTIVE_FLAG = 'Y'
AND NVL(PrmAllianceManager.END_DATE, SYSDATE + 1) > SYSDATE
AND PrmAllianceManager.START_DATE < SYSDATE]]>
  </SQLQuery>
