  <Variable
    Name="GroupType"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery>
    <![CDATA[SELECT tt.ID,
    tt.TARGET_TRACKING_NO,
    tt.CATALOG_NUMBER,
    tt.LONG_TITLE AS TITLE,
    ptg.ID AS GROUP_ID,
    ptg.GROUP_NAME, 
    jus.ID AS USER_ID,
    jus.USERNAME,
    ptgt.ADDED_DATE,
    tco.ID AS ACCOUNT_ID,
    tco.CONTENT_OWNER_NO AS ACCOUNT_NO,
    tco.NAME AS ACCOUNT_NAME,
    tt.OPEN_ACCESS,
    listagg(TraxTitleIndexInclusion.INDEX_NAME, ', ') within group (order by index_name) AS EXTERNAL_INDEXES
FROM TRAX_TITLE tt 
    LEFT OUTER JOIN TRAX_TITLE_INDEX_INCLUSION TraxTitleIndexInclusion
        ON tt.ID = TraxTitleIndexInclusion.TITLE_ID
    INNER JOIN PRM_TARGET_GROUP_TITLE ptgt
        INNER JOIN PRM_TARGET_GROUP ptg
            on ptgt.PRM_TARGET_GROUP_ID = ptg.ID
        ON tt.ID = ptgt.TITLE_ID
    LEFT OUTER JOIN TRAX_CONTENT_OWNER tco
        ON tt.CONTENT_OWNER_ID = tco.ID
    INNER JOIN JHS_USERS jus
        ON jus.username = :uname
WHERE ptg.GROUP_TYPE = :GroupType
AND ptgt.ADDED_DATE > TRUNC(SYSDATE - 45)
GROUP BY tt.ID,
    tt.TARGET_TRACKING_NO,
    tt.CATALOG_NUMBER,
    tt.LONG_TITLE,
    ptg.ID,
    ptg.GROUP_NAME, 
    jus.ID,
    jus.USERNAME,
    ptgt.ADDED_DATE,
    tco.ID,
    tco.CONTENT_OWNER_NO,
    tco.NAME,
    tt.OPEN_ACCESS]]>
  </SQLQuery>
