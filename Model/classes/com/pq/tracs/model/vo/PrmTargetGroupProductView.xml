  <SQLQuery>
    <![CDATA[SELECT PrmTargetGroupProduct.ADDED_DATE, 
       PrmTargetGroupProduct.CREATED_BY, 
       PrmTargetGroupProduct.CREATED_DATE, 
       PrmTargetGroupProduct.ID, 
       PrmTargetGroupProduct.LAST_UPDATED_BY, 
       PrmTargetGroupProduct.LAST_UPDATED_DATE, 
       PrmTargetGroupProduct.PRM_TARGET_GROUP_ID, 
       PrmTargetGroupProduct.PRM_TARGETED_PRODUCT_ID, 
       PrmTargetGroupProduct.REMOVED_DATE, 
       PrmTargetedProduct.ID AS ID1, 
       PrmTargetedProduct.PRD_ID, 
       PrmTargetedProduct.NAME, 
       PrmTargetedProduct.PRODUCT_FAMILY,
       JhsUsers.DISPLAY_NAME AS PRODUCT_MANAGER
FROM PRM_TARGET_GROUP_PRODUCT PrmTargetGroupProduct
    INNER JOIN PRM_TARGETED_PRODUCT PrmTargetedProduct
        LEFT OUTER JOIN PRM_TARGETED_PRODUCT_MANAGER PrmTargetedProductManager
            INNER JOIN PRM_ALLIANCE_MANAGER PrmAllianceManager
                INNER JOIN JHS_USERS JhsUsers
                    ON PrmAllianceManager.JHS_USERS_ID = JhsUsers.ID
                ON PrmTargetedProductManager.PRM_USER_ID = PrmAllianceManager.ID
            ON PrmTargetedProduct.ID = PrmTargetedProductManager.PRM_TARGETED_PRODUCT_ID
            AND NVL(PrmTargetedProductManager.REMOVED_DATE, SYSDATE) >= TRUNC(SYSDATE)
    ON PrmTargetGroupProduct.PRM_TARGETED_PRODUCT_ID = PrmTargetedProduct.ID]]>
  </SQLQuery>
